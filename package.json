{
  "name": "babycube",
  "author": "Lucas Madranges",
  "private": true,
  "workspaces": [
    "./apps/launcher",
    "./apps/minecraft/*"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint . --config eslint.config.cjs --ext .ts,.tsx,.js,.jsx",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "commitlint": "^19.8.1",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4"
  }
}
